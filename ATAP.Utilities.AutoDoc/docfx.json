{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "ATAP.Utilities.Http/**.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**", "*AutoDoc/_site/**" ],
          "src": "../"
        }
      ],
      "dest": "obj/docfx/api/netstandard2.0/",
      "disableGitFeatures": false,
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    },
    {
      "src": [
        {
          "files": [
            "ATAP.Utilities.Http/**.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**", "*AutoDoc/_site/**" ],
          "src": "../"
        }
      ],
      "dest": "obj/docfx/api/netcoreapp2.0/",
      "disableGitFeatures": false,
      "properties": {
        "TargetFramework": "netcoreapp2.0"
      }
    }
  ],
  "merge": {
    "content": [
      {
        "files": "*.yml",
        "src": "obj/docfx/api/netcoreapp2.0"
      },
      {
        "files": "*.yml",
        "src": "obj/docfx/api/netstandard2.0"
      }
    ],
    "fileMetadata": {
      "platform": {
        "obj/docfx/api/netstandard2.0/*.yml": [
          "netstandard2.0"
        ],
        "obj/docfx/api/netcoreapp2.0/*.yml": [
          "netcoreapp2.0"
        ]
      }
    },
    "dest": "api"
  },


      "build": {
        "content": [
          {
            "files": [
              "**.yml",
              "index.md",
              "**/toc.yml",
              "**/index.md",
              "toc.md"
            ],
            "src": "obj/docfx/api",
            "dest": "Api"

          },
          {
            "files": [

              "ReadMe.md",
              "**/Docs/**/*.md"
            ],
            "exclude": [ "**AutoDoc/Docs/**", "**AutoDoc/_site/**" ],
            "src": "../",
            "dest": "Articles"
          },
          {
            "files": [
              "Index.md"
            ]

          }

        ],
        "resource": [
          {
            "files": [
              "images/**",
              "svg/**"
            ]
          }
        ],
        "globalMetadata": {
          "_appTitle": "ATAP Utilities",

          "_appLogoPath": "images/ataplogo1inch.bmp",
          "_enableSearch": true,
          "_enableNewTab": true,
          "_disableNavbar": false,
          "_disableBreadcrumb": false,
          "_disableToc": false,
          "_disableAffix": false,
          "_disableContribution": false

        },
        "overwrite": [
          {
            "files": [
              "obj/docfx/api/**.md"
            ],
            "exclude": [
              "obj/**",
              "_site/**"
            ]
          }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
          "default",
          "templates/AutoDocTemplate01"
        ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
      }
    }
