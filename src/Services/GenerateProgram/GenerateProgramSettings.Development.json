{
  "Logging": {
    "PathFormat": "C:/DevLogs/Serilog/AConsole01StartupLog.{Date}.log",
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "themes": "{Serilog.Sinks.SystemConsole.Themes.SystemConsoleTheme.AnsiConsoleTheme.Code}"
        }
      },
      {
        "Name": "Debug",
        "Args": {
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}"
        }
      }
    ],
    "WriteTo:Async": {
      "Name": "Async",
      "Args": {
        "configure": [
          {
            "Name": "File",
            "Args": {
              "path": "C:\\DevLogs\\AConsole01\\Serilogs\\log.txt",
              "outputTemplate": "{Timestamp:o} [{Level:u3}] ({Application}/{MachineName}/{ThreadId}) {Message}{NewLine}{Exception}",
              "rollingInterval": "RollingInterval.Day"
            }
          }
        ]
      }
    },
    "Enrich": [ "FromLogContext", "WithThreadId" ],
    "Properties": {
      "Application": "AConsole01"
    }
  },
  "OutputDirectory": "@\"../../../Artefacts/\"",
    "<GProjectType" "Sdk=\"Microsoft.NET.Sdk\">",

  "HostedServiceUsings": [
    "Microsoft.Extensions.Localization",
    "Microsoft.Extensions.Options",
    "Microsoft.Extensions.Configuration",
    "Microsoft.Extensions.Logging",
    "Microsoft.Extensions.Logging.Abstractions",
    "Microsoft.Extensions.DependencyInjection",
    "Microsoft.Extensions.Hosting",
    "Microsoft.Extensions.Hosting.Internal"
  ],

  "HostedServiceUsingsPerServiceType": {
    "ORMAccess": [],
    "StdIn": [],
    "StdOut": [],
    "Serialization: []"
  },

  "CompilationUnitBaseNames": [
    "Program",

    "StdInHandler",
    "FileSystemGraphToDB",
    "FileSystemToObjectGraph",
    "FileSystemWatchers",
    "ConsoleMonitor",
    "ConsoleSink",
    "ConsoleSource"
  ],
  "StdInHandler": [
    "GUsings",
    :,
    []
  ],
  "NamespaceNames": "CompilationUnitBaseNames",
  "EpiClassNames": "ClassHostedServiceNames",
  "ClassHostedServiceNames": [
    "StdInHandler",
    "FileSystemGraphToDB",
    "FileSystemToObjectGraph",
    "FileSystemWatchers",
    "ConsoleMonitor",
    "ConsoleSink",
    "ConsoleSource"
  ],
  "ClassNameFromClassHostedServiceNames": "{ClassNameSubstitutionPattern}Service",
  "DataClassNames": "{ClassHostedServiceNamesSubstitutionPattern}Data",
  "HostedServiceAdditionalInterfaceNames": [],
  "InterfaceNameFromClassNameTransform": "I{ClassNameSubstitutionPattern}",
  "ClassHostedServicePropertyNames": [
    "{ClassHostedServiceNamesSubstitutionPattern}Data[varname=serviceData]",
    "ClassHostedServicePropertyConstructorInjectedNames",
    :,
    [
      "StringLocalizerFactory",
      "LoggerFactory",
      "HostEnvironment",
      "HostConfiguration",
      "HostLifetime",
      "AppConfiguration",
      "HostApplicationLifetime"
    ],
    "ClassHostedServicePropertyConstructorInjectedDerivedNames",
    :,
    [
      "Logger",
      "UiLocalizer",
      "ExceptionLocalizer",
      "DebugLocalizer"
    ]

  ],
  "ClassHostedServicePropertyNamesPerService": {
    "StdInHandler": [
      "FileSystemGraphToDB",
      "FileSystemToObjectGraph",
      "ConsoleSinkHosted",
      "ConsoleSourceHoste"
    ],
    "IFileSystemToObjectGraphService": [
      "ConsoleSinkHosted",
      "ConsoleSourceHosted"
    ],
    "IFileSystemGraphToDBService": [
      "ConsoleSinkHosted",
      "ConsoleSourceHosted"
    ]
  },
  "ClassHostedServiceConstructorBodyFileName": "HostedServiceConstructorBody",
  "ClassHostedServiceConstructorBodyFileSuffix": "template"
}
