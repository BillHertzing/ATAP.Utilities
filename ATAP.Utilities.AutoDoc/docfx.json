{
      "metadata": [
        {
          "src": [
            {
              "files": ["ATAP.Utilities.Http/*.csproj","ATAP.Utilities.BuildTooling.*/*.csproj","ATAP.Utilities.AutoDoc/*.csproj"],
              "exclude": [ "**/bin/**", "**/obj/**"],
              "src": "../"
            }
          ],
          "dest": "obj/docfx/api/netstandard2.0/",
          "disableGitFeatures": false,
          "properties": {
            "TargetFramework": "netstandard2.0"
          }
        },
        {
          "src": [
            {
              "files": ["ATAP.Utilities.Http/*.csproj","ATAP.Utilities.BuildTooling.*/*.csproj","ATAP.Utilities.AutoDoc/*.csproj"],
              "exclude": [ "**/bin/**", "**/obj/**" ],
              "src": "../"
            }
          ],
          "dest": "obj/docfx/api/netcoreapp2.0/",
          "disableGitFeatures": false,
          "properties": {
            "TargetFramework": "netcoreapp2.0"
          }
        },
        {
          "src": [
            {
              "files": ["ATAP.Utilities.Http/*.csproj","ATAP.Utilities.BuildTooling.*/*.csproj","ATAP.Utilities.AutoDoc/*.csproj"],
              "exclude": [ "**/bin/**", "**/obj/**"],
              "src": "../"
            }
          ],
          "dest": "obj/docfx/api/net471/",
          "disableGitFeatures": false,
          "properties": {
            "TargetFramework": "net471"
          }
        }
      ],

  "build": {
    "content": [
      {
        "files": [
          "**.yml",
          "index.md",
          "toc.md",
          "**/*.yml",
          "**/toc.yml",
          "**/index.md"
        ],
        "src": "obj/docfx/api",
        "dest": "Api/"
      },
      {
        "files": [
          "index.md", "toc.yml"
        ],
        "src": "../",
        "dest": "./"
      },
      {
        "files": [
          "ReadMe.md"
        ],
        "src": "../",
        "dest": "ReadMes/"
      },
      {
        "files": [
          "*.md",
          "*.yml"
        ],
        "src": "../Documentation",
        "dest": "Documentation/"
      }

    ],
    "resource": [
      {
        "files": [
          "images/**",
          "svg/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "obj/docfx/api/**.md"
        ],
        "exclude": [ "**/bin/**", "**/obj/**"],
      },
    ],
    "globalMetadata": {
      "_appTitle": "ATAP Utilities",

      "_appLogoPath": "images/ataplogo1inch.bmp",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableNavbar": false,
      "_disableBreadcrumb": false,
      "_disableToc": false,
      "_disableAffix": false,
      "_disableContribution": true

    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "dest": "../Docs",
    "template": [
      "default",
      "templates/AutoDocTemplate01"
    ],
    "merge": {
      "content": [
        {
          "files": "*.yml",
          "src": "obj/docfx/api/net471"
        },
        {
          "files": "*.yml",
          "src": "obj/docfx/api/netcoreapp2.0"
        },
        {
          "files": "*.yml",
          "src": "obj/docfx/api/netstandard2.0"
        }
      ],
      "fileMetadata": {
        "platform": {
          "obj/docfx/api/netstandard2.0/*.yml": [
            "netstandard2.0"
          ],
          "obj/docfx/api/netcoreapp2.0/*.yml": [
            "netcoreapp2.0"
          ],
		  "obj/docfx/api/net471/*.yml": [
            "net471"
          ]
        }
      },
      "dest": "obj/docfx/api/"
    },
    "postProcessors": [ "ExtractSearchIndex" ],
    "markdownEngineName": "dfm-latest",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
