
<#+ // Start of feature control block.
string GenerateConstructorDeclarationArgumentGroups(GConstructorDeclarationArgumentGroup ag)
{ var sb = new StringBuilder();
var args = new string[ag.GConstructorArguments.Count];
for (var i = 0; i< ag.GConstructorArguments.Count; i++ ) {
  args[i] = ag.GConstructorArguments[i].GType + " " + ag.GConstructorArguments[i].GName;
}
sb.Append(String.Join(",",args)) #>
<#= sb.Tostring() #>
<#+ } // End of feature control block. #>
