{
      "metadata": [
        {
          "src": [
            {
              "files": [
                "ATAP.Utilities.Http/**.csproj"
              ],
              "exclude": [ "**/bin/**", "**/obj/**", "*AutoDoc/_site/**" ],
              "src": "../"
            }
          ],
          "dest": "obj/docfx/api/netstandard2.0/",
          "disableGitFeatures": false,
          "properties": {
            "TargetFramework": "netstandard2.0"
          }
        },
        {
          "src": [
            {
              "files": [
                "ATAP.Utilities.Http/**.csproj"
              ],
              "exclude": [ "**/bin/**", "**/obj/**", "*AutoDoc/_site/**" ],
              "src": "../"
            }
          ],
          "dest": "obj/docfx/api/netcoreapp2.0/",
          "disableGitFeatures": false,
          "properties": {
            "TargetFramework": "netcoreapp2.0"
          }
        }
      ],

  "build": {
    "content": [
      {
        "files": [
          "**.yml",
          "index.md",
          "toc.md",
          "**/toc.yml",
          "**/index.md"
        ],
        "src": "obj/docfx/api",
        "dest": "Api"

      },
      {
        "files": [
          "ReadMe.md",
          "**/Docs/**/*.md",
          "**/*.yml"
        ],
        "exclude": [ "**/obj/**", "**/_site/**", "**/_exported_templates/**" ],
        "src": "../",
        "dest": "Articles"
      },
      {
        "files": [
          "index.md",
          "toc.yml"
        ],
        "src": "./"
      }

    ],
    "resource": [
      {
        "files": [
          "images/**",
          "svg/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "obj/docfx/api/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      },
      {
        "files": [
          "Articles/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "ATAP Utilities",

      "_appLogoPath": "images/ataplogo1inch.bmp",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableNavbar": false,
      "_disableBreadcrumb": false,
      "_disableToc": false,
      "_disableAffix": false,
      "_disableContribution": false

    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "dest": "_site",
    "template": [
      "default",
      "templates/AutoDocTemplate01"
    ],
    "merge": {
      "content": [
        {
          "files": "*.yml",
          "src": "obj/docfx/api/netcoreapp2.0"
        },
        {
          "files": "*.yml",
          "src": "obj/docfx/api/netstandard2.0"
        }
      ],
      "fileMetadata": {
        "platform": {
          "obj/docfx/api/netstandard2.0/*.yml": [
            "netstandard2.0"
          ],
          "obj/docfx/api/netcoreapp2.0/*.yml": [
            "netcoreapp2.0"
          ]
        }
      },
      "dest": "obj/docfx/api/merged"
    },
    "postProcessors": [ "ExtractSearchIndex" ],
    "markdownEngineName": "dfm-latest",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
