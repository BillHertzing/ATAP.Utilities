<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
  <StartupObject>GenerateProgram.PService03</StartupObject>
  </PropertyGroup>

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>net5.0;</TargetFrameworks> <!-- netcoreapp3.1 net47-->
    <DefaultNamespace>GenerateProgram</DefaultNamespace>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\ATAP.Utilities.Philote.DefaultConfiguration\ATAP.Utilities.Philote.DefaultConfiguration.csproj" />
    <ProjectReference Include="..\ATAP.Utilities.Philote.Interfaces\ATAP.Utilities.Philote.Interfaces.csproj" />
    <ProjectReference Include="..\ATAP.Utilities.Philote\ATAP.Utilities.Philote.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Artifacts\Resources\" />
  </ItemGroup>

<PropertyGroup>
  <DefaultItemExcludes>Artifacts\**</DefaultItemExcludes>
  <DefaultItemExcludes>$(DefaultItemExcludes);**\Resources\**</DefaultItemExcludes>
</PropertyGroup>

  <!-- ATAP Utility library for collection extensions (for Dictionary AddRange) -->
  <ItemGroup>
    <ProjectReference Include="..\ATAP.Utilities.Collection.Extensions\ATAP.Utilities.Collection.Extensions.csproj" />
  </ItemGroup>


  <!-- Packages necessary to run the ASP.Net Core Generic Host and web server hosts Server -->
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration" />
    <PackageReference Include="Microsoft.Extensions.Configuration.CommandLine" />
    <PackageReference Include="Microsoft.Extensions.Configuration.EnvironmentVariables" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" />
    <PackageReference Include="Microsoft.Extensions.Hosting" />
    <PackageReference Include="Microsoft.Extensions.Localization" />
    <PackageReference Include="Microsoft.Extensions.Logging" />
  </ItemGroup>

  <!-- Packages to persist data to QuickGraph -->
  <ItemGroup>
    <PackageReference Include="YC.QuickGraph" />
    <PackageReference Include="DotNet.Contracts" />
    <PackageReference Include="FSharp.Core" />
    <PackageReference Include="FSharpx.Collections.Experimental" />
  </ItemGroup>

  <ItemGroup>
    <None Update="CResourceUnitPreambleTemplate.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
