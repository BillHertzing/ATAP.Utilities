{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../"
        }
      ],
      "dest": "obj/docfx/api",
      "properties": {
        "TargetFramework": "netstandard2.0",
        "TargetFramework": "netcoreapp2.0"
      },
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**.yml",
          "index.md",
          "**/toc.yml",
          "**/index.md",
          "toc.md"
        ],
        "src": "obj/docfx/api",
        "dest": "Api"

      },
      {
        "files": [

          "ReadMe.md",
          "**/Docs/**/*.md"
        ],
        "exclude": [ "**AutoDoc/Docs/**" ],
        "src": "../",
        "dest": "Articles"
      }

    ],
    "resource": [
      {
        "files": [
          "images/**",
          "svg/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "ATAP Utilities",
      "_disableContribution": true

    },
    "overwrite": [
      {
        "files": [
          "obj/temp/docfx/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "Docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/AutoDocTemplate01"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
