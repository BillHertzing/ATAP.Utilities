{

    "omnisharp.enableEditorConfigSupport": true,
    "omnisharp.useGlobalMono": "never",
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "testExplorer.addToEditorContextMenu": true,
    "nxunitExplorer.logpanel": true,
    "nxunitExplorer.modules": [
        "tests/**/bin/**/**/*.{dll,exe}"
    ],
    "yaml.schemas": {},
    "cSpell.enableFiletypes": [
        "sql"
    ],
    "dotnet-test-explorer.testProjectPath":"tests/**/*Tests.csproj",
    "cSpell.allowCompoundWords": true,
    "cSpell.userWords": [
        "Finalizer",
        "GDOT",
        "GHBS",
        "GHHS",
        "Guids",
        "IORM",
        "Itenso",
        "KVPs",
        "MSMQ",
        "Newtonsoft",
        "Philote",
        "Propertys",
        "Serilog",
        "Usings",
        "Xunit",
        "atap",
        "datas",
        "localizers",
        "number",
        "of",
        "graphid",
        "whertzing"
    ],
    "cSpell.words": [
        "AOEII",
        "globbing",
        "Ninject",
        "Prefixs",
        "serializers"
    ],
    "markdownlint.config": {
        "MD028": false,
        "MD025": {
            "front_matter_title": ""
        }
    }
}
