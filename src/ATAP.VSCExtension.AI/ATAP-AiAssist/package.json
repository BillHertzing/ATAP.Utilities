{
  "name": "ATAP-AIAssist",
  "displayName": "ATAP-AIAssist",
  "description": "AI code helper",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./_generated/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ATAP-AIAssist.copyToSubmit",
        "title": "copyToSubmit"
      },
      {
        "command": "ATAP-AIAssist.removeRegion",
        "title": "Remove #region lines"
      },
      {
        "command": "ATAP-AIAssist.processPs1Files",
        "title": "process all .ps1 files in a workspace (Requires command ID as argument)"
      }
    ],
    "keybindings": [
      {
          "command": "ATAP-AIAssist.processPs1Files",
          "key": "ctrl+l ctrl+p"
      }
  ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./_generated/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  }
}
