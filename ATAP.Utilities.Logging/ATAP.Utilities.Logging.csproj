    <InternalsVisibleTo>ATAP.Utilities.Tcp</InternalsVisibleTo>
    <InternalsVisibleTo>ATAP.Utilities.Tcp.UnitTests</InternalsVisibleTo>
    <InternalsVisibleTo>ATAP.Utilities.UnitTest</InternalsVisibleTo>
