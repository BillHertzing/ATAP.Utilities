{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "C:\\Dropbox\\whertzing\\GitHub\\ATAP.Utilities\\ATAP.Utilities.BuildTooling.CSharp"
        }
      ],
      "dest": "obj/temp/docfx",
      "properties": {
        "TargetFramework": "netstandard2.0"
      },
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**.yml",
          "index.md"
        ],
        "src": "obj/temp/docfx"
      },
      {
        "files": [
          "*.md"
        ]
      },
      {
        "files": [
          "DocSrc/articles/**.md",
          "DocSrc/articles/**/toc.yml",
          "DocSrc/toc.yml",
          "DocSrc/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "DocSrc/images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "obj/temp/docfx/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "Docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
